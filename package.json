{
  "name": "image-min",
  "version": "0.0.1",
  "description": "图片压缩工具",
  "main": "main.js",
  "scripts": {
    "build:test": "cross-env NODE_ENV=development node webpack-build.js",
    "build:web": "cross-env NODE_ENV=production node webpack-build.js",
    "build:web:dev": "cross-env NODE_ENV=development node webpack-build.js",
    "build:mac": "electron-builder --dir --x64 --platform darwin",
    "build:linux": "electron-builder --dir --x64 --platform linux",
    "build:win": "electron-builder --dir --x64 --platform win32",
    "dev": "cross-env NODE_ENV=development electron .",
    "pack:linux": "electron-builder --x64 --platform linux",
    "pack:mac": "electron-builder --x64 --platform darwin",
    "pack:win": "electron-builder --x64 --platform win32",
    "start": "cross-env NODE_ENV=production electron ."
  },
  "author": "duzefeng",
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd": "^3.11.6",
    "axios": "^0.19.2",
