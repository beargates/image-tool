    "fluent-ffmpeg": "^2.1.2",
    "fs": "^0.0.1-security",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^9.0.0",
    "prop-types": "^15.7.2",
    "qiniu": "^7.3.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "util": "^0.12.3",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
